{
  "name": "@k11i/bolt-dynamodb",
  "version": "1.1.4",
  "description": "DynamoDB-backed InstallationStore implementation for bolt-js",
  "author": {
    "name": "KOMIYA Atsushi",
    "url": "https://github.com/komiya-atsushi"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/komiya-atsushi/slack-bolt-aws.git"
  },
  "bugs": {
    "url": "https://github.com/komiya-atsushi/slack-bolt-aws/issues"
  },
  "keywords": [
    "slack",
    "dynamodb"
  ],
  "engines": {
    "node": ">=18"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "files": [
    "dist/*.*"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --sourcemap",
    "compile": "tsc",
    "test:attw": "attw --pack ."
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.621.0",
    "@slack/logger": "^4.0.0",
    "@slack/oauth": "^2.6.2 || ^3.0.1"
  }
}
